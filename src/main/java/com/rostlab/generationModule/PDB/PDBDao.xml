<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rostlab.generationModule.uniprot.UniProtDAO">

    <resultMap id="pdbEntityResultMap" type="com.rostlab.generationModule.PDB.PDBEntity">
        <result column="acc_id" property="acc_id" />
        <result column="dssp" property="dssp" />
    </resultMap>

    <select id="findByAccId" resultType="pdbEntityResultMap">
        select acc_id, dssp from pdb where acc_id = #{acc_id}
    </select>

    <select id="findByDssp" resultType="pdbEntityResultMap">
        select acc_id, dssp from pdb where dssp = #{dssp}
    </select>

    <insert id = "insert" parameterType = "PDBEntity">
        INSERT INTO pdb (acc_id, dssp)
        VALUES (#{entity.acc_id}, #{entity.dssp});
    </insert>
</mapper>